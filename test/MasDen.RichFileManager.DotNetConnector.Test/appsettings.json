{
    "Filemanager": {
        "rootPath": "TestData",
        "iconDirectory": "scripts\\filemanager\\images\\fileicons\\",
        "security": {
          "readOnly": false,
          "extensions": {
            "policy": "ALLOW_ALL",
            "ignoreCase": true,
            "restrictions": [
              "jpg",
              "jpe",
              "jpeg",
              "gif",
              "png"
            ]
          }
        },
      "upload": {
        "multiple": true,
        "maxNumberOfFiles": 3,
        "paramName": "files",
        "chunkSize": true,
        "fileSizeLimit": 10
      }
    }
}