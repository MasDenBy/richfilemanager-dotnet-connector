{
    "Filemanager": {
        "rootPath": "Files",
        "iconDirectory": "scripts\\filemanager\\images\\fileicons\\",
        "options": {
            "culture": "en",
            "theme": "flat-dark",
            "defaultViewMode": "grid",
            "localizeGUI": true,
            "showFullPath": true,
            "showTitleAttr": true,
            "showConfirmation": true,
            "browseOnly": true,
            "clipboard": true,
            "searchBox": true,
            "listFiles": true,
            "fileSorting": "NAME_ASC",
            "folderPosition": "bottom",
            "quickSelect": true,
            "charsLatinOnly": true,
            "splitterWidth": 100,
            "splitterMinWidth": 200,
            "logger": true,
            "capabilities": "capabilities"
        },
        "security": {
            "allowFolderDownload": true,
            "allowChangeExtensions": true,
            "allowNoExtension": true,
            "normalizeFilename": true
        },
        "upload": {
            "multiple": true,
            "maxNumberOfFiles": 3,
            "paramName": "files",
            "chunkSize": true,
            "fileSizeLimit": 10,
            "policy": "DISALLOW_ALL",
            "restrictions": "restrictions"
        }
    }
}